{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx/client-side-web-part-manifest.schema.json",
  "id": "dfd4f92b-ad79-4633-973a-b44472cc3e6d",
  "alias": "SpFxManifestFileWpDemoWebPart",
  "componentType": "WebPart",

  // The "*" signifies that the version should be taken from the package.json
  "version": "*",
  "manifestVersion": 2,

  // If true, the component can only be installed on sites where Custom Script is allowed.
  // Components that allow authors to embed arbitrary script code should set this to true.
  // https://support.office.com/en-us/article/Turn-scripting-capabilities-on-or-off-1f2c515f-5d7e-448a-9fd7-835da935584f
  "requiresCustomScript": false,
  "supportedHosts": ["SharePointWebPart"],
  
  //Enable Full Column Width Section Layout
  "supportsFullBleed": true,

  //Disable on Classic SharePoint Site
  "disabledOnClassicSharepoint": true,

  //Hide from Toolbox
  "hiddenFromToolbox": false,
  
  //Inherit theme properties
  "supportsThemeVariants": true
,

  "preconfiguredEntries": [{
    // Define WP Group from Default to Text Media Content
    "groupId": "cf066440-0614-43d6-98ae-0b31cf14c7c3", 
    "group": { "default": "Text, media, and content"},

    "title": { "default": "Manifest File Demo WebPart" },
    "description": { "default": "SPFx Manifest File Demo WebPart" },
    
    // Change Icons
    "officeFabricIconFontName": "SkypeCircleSlash",
        // OR From SharePoint Assest
        //"iconImageUrl": "https://vxdpg.sharepoint.com/sites/LFC/SiteAssets/hello.png",

    "properties": {
      "description": "SPFx-Manifest-File-WP-Demo"
    }
  }]
}
